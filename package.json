{
  "name": "rojas-camarena-papaclick",
  "version": "1.0.0",
  "description": "Sistema de clasificaciÃ³n de variedades de papa usando IA - Arquitectura 4 capas",
  "main": "electron-main.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pmv1": "cd PMV1 && node app.js",
    "pmv1:dev": "cd PMV1 && nodemon app.js",
    "install:pmv1": "cd PMV1 && npm install",
    "config:atlas": "node PMV1/scripts/configurar-atlas.js",
    "init:data": "cd PMV1 && node scripts/inicializar-datos.js",
    "build:exe": "pkg PMV1/app.js --targets node18-win-x64 --out-path dist",
    "build:all": "pkg PMV1/app.js --targets node18-win-x64,linux-x64,macos-x64 --out-path dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "electron:dev": "cross-env PORT=34567 electron .",
    "build:electron": "electron-builder --win --x64",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "tensorflow",
    "clasificacion",
    "papas",
    "inteligencia-artificial",
    "machine-learning"
  ],
  "author": "Rojas Camarena",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "moment": "^2.30.1",
    "mongodb": "^6.20.0",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "serverless-http": "^3.0.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^26.6.10",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "pkg": {
    "assets": [
      "PMV1/public/**/*",
      "PMV1/vista/**/*",
      "PMV1/web_model/**/*",
      "PMV1/uploads/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IosmilRojas/ROJAS-CAMARENA-PAPANATIVA"
  },
  "bugs": {
    "url": "https://github.com/IosmilRojas/ROJAS-CAMARENA-PAPANATIVA/issues"
  },
  "homepage": "https://github.com/IosmilRojas/ROJAS-CAMARENA-PAPANATIVA#readme",
  "build": {
    "appId": "com.rojascamarena.papaclick",
    "files": [
      "PMV1/**/*",
      "electron-main.js",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "artifactName": "${productName}-Setup-${version}.exe"
    }
  }
}
